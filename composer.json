{
    "name": "naprstek/laminas-session-validator",
    "description": "aditional session validators",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf",
        "laminas",
        "session",
        "validator"
    ],
    "require": {
        "php": "^7.4",
        "laminas/laminas-session": "^2.8 || ^3.0",
        "laminas/laminas-http": "^2.8",
        "jenssegers/agent": "^2.6",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Session\\Validator\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
    }
}
